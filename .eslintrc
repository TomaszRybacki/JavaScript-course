{
    "extends": "airbnb-base",
    "parserOptions": {
    "ecmaVersion": 5
    },
    "env": {
    "node": true,
    "browser": true,
    "jquery": true
    },
    "rules": {
    "comma-dangle": ["error", "never"],
    "no-var": [0],
    "func-names": [0],
    "no-restricted-globals": [0],
    "prefer-template": [0],
    "prefer-arrow-callback": [0],
    "no-unused-vars": ["error", {"vars": "local", "args": "none"}],
    "react/jsx-filename-extension": [0],
    }

}
